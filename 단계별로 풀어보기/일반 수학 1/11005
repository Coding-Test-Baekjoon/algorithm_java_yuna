import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
        
	        Scanner sc = new Scanner(System.in);
	        
	        int n = sc.nextInt();  // 10진법 수
	        int b = sc.nextInt();  // 진법
	        
	        String answer = Integer.toString(n, b).toUpperCase();
	        System.out.println(answer);
        
	}
}
