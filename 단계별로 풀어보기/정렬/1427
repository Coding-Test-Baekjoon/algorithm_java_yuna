import java.util.*;
public class Main {
	public static void main(String[] args) {
 
        	Scanner sc = new Scanner(System.in);
        	String n = sc.next();
        	char[] nArr = n.toCharArray();
        	sc.close();
        
        	Arrays.sort(nArr);

	        for (int i = nArr.length - 1; i >= 0; i--) {
            		System.out.print(nArr[i]);
        	}
    	
     }
}
